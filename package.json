{
  "name": "rocketclimb-icons",
  "version": "0.1.0",
  "description": "React / React Native powered by tailwind",
  "keywords": [
    "icons",
    "react",
    "react-native",
    "tailwind",
    "nativewind"
  ],
  "homepage": "https://github.com/rocketclimb/rocketicons#readme",
  "bugs": {
    "url": "https://github.com/rocketclimb/rocketicons/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rocketclimb/rocketicons.git"
  },
  "license": "MIT",
  "author": "amorimjj",
  "main": "dist/index.js",
  "workspaces": [
    "packages/sh",
    "packages/core",
    "packages/tailwind",
    "packages/gen",
    "packages/icons",
    "packages/ignition"
  ],
  "scripts": {
    "build-all": "npm run build --workspaces --if-present",
    "clean-all": "npm run clean --workspaces --if-present",
    "prepare": "husky && npm run clean-all && npm run build-all",
    "test-all": "npm run test --workspaces --if-present",
    "format-all": "npm run format --workspaces --if-present",
    "lint-all": "npm run lint --workspaces --if-present"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@jest/globals": "^29.7.0",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "eslint-plugin-react": "^7.34.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "nativewind": "2.0.11",
    "prettier": "^3.2.5",
    "release-it": "17.1.1",
    "tailwindcss": "3.4.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-replace-paths": "^0.0.14",
    "typescript": "^5.4.3"
  },
  "peerDependencies": {
    "@babel/preset-env": "7.24.3",
    "nativewind": ">=2.0.11",
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0",
    "react-native-svg": ">=15.1.0"
  },
  "lint-staged": {
    "*.+(ts|tsx)": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.+(json|css|md)": [
      "prettier --write"
    ]
  }
}
