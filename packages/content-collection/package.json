{
  "name": "@rocketclimb/content-collections",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts -d dist",
    "clean": "rocketicons-sh rm -fR dist && rocketicons-sh rm -fR *.tsbuildinfo",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint .",
    "rebuild": "npm run clean && npm run build",
    "test": "jest ./src --passWithNoTests",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@parcel/watcher": "^2.3.0",
    "camelcase": "^8.0.0",
    "esbuild": "^0.19.5",
    "fast-glob": "^3.3.2",
    "gray-matter": "^4.0.3",
    "micromatch": "^4.0.5",
    "pluralize": "^8.0.0",
    "yaml": "^2.3.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/micromatch": "^4.0.6",
    "@types/node": "^20.9.0",
    "@types/pluralize": "^0.0.33",
    "tsup": "^7.2.0",
    "tsx": "^4.1.1",
    "typescript": "^5.3.3"
  }
}
