{
  "name": "@rocketicons/core",
  "version": "0.15.0",
  "license": "MIT",
  "main": "./dist/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json ./tsconfig.types.json && tsconfig-replace-paths",
    "postbuild": "rocketicons-sh cp \"dist-esm/**/*.js\" \"dist/**/*.mjs\"",
    "clean": "rocketicons-sh rm -fR dist dist-esm && rocketicons-sh rm -fR *.tsbuildinfo",
    "clean:o": "rocketicons-sh rm -fR node_modules",
    "format": "prettier --check . --write --ignore-path ../../.gitignore",
    "lint": "eslint src/**/*",
    "rebuild": "npm run clean && npm run build",
    "test": "jest ./src",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "rocketicons-sh": "*"
  }
}
