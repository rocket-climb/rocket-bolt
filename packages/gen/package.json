{
  "name": "@rocket-climb/bolt-gen",
  "version": "0.1.0",
  "license": "MIT",
  "main": "./dist/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "prebuild": "npm run fetch",
    "build": "npx ts-node ./src/build.ts",
    "postbuild": "npm run copy-files",
    "clean": "bolt-sh rm -fR dist core .turbo *.tsbuildinfo",
    "clean:o": "bolt-sh rm -fR .turbo node_modules",
    "copy-files": "bolt-sh cp -fR ../core/dist/* ../icons/core/ && bolt-sh cp -fR ../tailwind/dist/* ../icons/tailwind/",
    "fetch": "npx ts-node ./src/fetcher.ts",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint .",
    "rebuild": "npm run clean && npm run build",
    "refetch": "npx ts-node ./src/fetcher.ts --force",
    "test": "jest ./src --passWithNoTests",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@esm2cjs/p-queue": "7.3.0",
    "@primer/octicons": "^19.9.0",
    "@rocket-climb/bolt-core": "*",
    "@types/react": "^18.2.72",
    "@types/react-dom": "^18.2.22",
    "cheerio": "^1.0.0-rc.12",
    "feather-icons": "^4.29.1",
    "find-package": "^1.0.0",
    "glob-promise": "^6.0.5",
    "ionicons": "^7.3.0",
    "ionicons-5": "npm:ionicons@5",
    "lucide-static": "^0.363.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native-svg": "15.1.0",
    "svgo": "^3.2.0"
  },
  "engines": {
    "npm": ">=6.0.0"
  }
}
